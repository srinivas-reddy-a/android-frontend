            android:name="com.akfrontend.arraykart.SearchPage.SearchPageActivity"
            android:exported="false"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateVisible"/>
        <activity
            android:name="com.akfrontend.arraykart.AddressActivity.AddressFormActivity"
            android:exported="false"
