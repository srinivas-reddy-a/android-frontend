        <activity
            android:name="com.akfrontend.arraykart.HelpCenterActivity.HelpCenterActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.akfrontend.arraykart.RatingReviewPage.AllReviewActivity"
