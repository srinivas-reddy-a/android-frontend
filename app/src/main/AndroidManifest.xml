<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.arraykart">
    <!-- userperm -->
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/arraykart_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/arraykart_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.Arraykart"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".MyOrder.OrderPlacedPage"
            android:exported="false" />
        <activity
            android:name=".AddressActivity.AddressEditForm"
            android:exported="false" />
        <activity
            android:name=".ProductDetailAboutListing.ProductTableDetailPage"
            android:exported="false" />
        <activity
            android:name=".Community.CommunityActivity"
            android:exported="false"
            android:label="@string/title_activity_community"
            android:theme="@style/Theme.Arraykart.NoActionBar" />
        <activity
            android:name=".HelpCenterActivity.HelpCenterActivity"
            android:exported="false" />
        <activity
            android:name=".RatingReviewPage.AllReviewActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".RatingReviewPage.RatingActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".NotificationPage.NotificationActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".UserProfile.ProfileEditPageActivity"
            android:exported="true"
            android:screenOrientation="portrait" />
        <activity
            android:name=".UserProfile.UserProfileActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".SearchPage.SearchPageActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".AddressActivity.AddressFormActivity"
            android:exported="false"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name=".AddressActivity.MyAddressActivity"
            android:exported="false"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name=".DeliveryPage.DeliveryActivity"
            android:exported="false"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name=".WishList.WishListActivity"
            android:exported="false"
            android:label="Wish List"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name=".MyOrder.OrderDetail"
            android:exported="false"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name=".Sort.sortPage"
            android:exported="false"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name=".Filter.FilterPage"
            android:exported="false"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name=".SignUP"
            android:exported="false"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name=".Signin"
            android:exported="false"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name=".MyOrder.MyOrder"
            android:exported="false"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name=".MyCart.MYCartActivity"
            android:exported="false"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name=".ProductDetailActivity"
            android:exported="false"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name=".homeCategoryProduct.allItemOfSingleProduct.ItemsForSingleProduct"
            android:exported="false"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name=".homeCategoryProduct.moreProductCategory.moreCategoryProducts"
            android:exported="false"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name=".HomeNavigationActivity"
            android:exported="false"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.Arraykart.NoActionBar"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.facebook.FacebookActivity"
            android:configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation"
            android:label="@string/app_name"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.facebook.CustomTabActivity"
            android:exported="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data android:scheme="@string/fb_login_protocol_scheme" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/facebook_app_id" />
    </application>

</manifest>