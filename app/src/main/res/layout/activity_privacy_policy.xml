                    android:textSize="@dimen/heading_size" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="20sp"
                    android:text="@string/pp_para1"
                    android:textColor="@color/text"
                    android:textSize="@dimen/heading_size" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
